<% language_tabs = current_page.data.language_tabs %>
<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title><%= current_page.data.title || "Wootric Documentation" %> | Wootric</title>
    <%= favicon_tag 'favicon-32x32.png' %>

    <%= stylesheet_link_tag :screen, media: :screen %>
    <%= stylesheet_link_tag :print, media: :print %>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <% if current_page.data.search %>
      <%= javascript_include_tag  "all" %>
    <% else %>
      <%= javascript_include_tag  "all_nosearch" %>
    <% end %>

    <% if language_tabs %>
      <script>
        $(function() {
          setupLanguages(<%= language_tabs.map{ |lang| lang.is_a?(Hash) ? lang.keys.first : lang }.to_json %>);
        });
      </script>
    <% end %>

    <!-- Google Tag Manager -->
    <script>
      (function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
      new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
      j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
      'https://www.googletagmanager.com/gtm.js?id='+i+dl;f.parentNode.insertBefore(j,f);
      })(window,document,'script','dataLayer','GTM-PF46GT3');
    </script>
    <!-- End Google Tag Manager -->
  </head>

  <body class="<%= page_classes %>">
    <!-- Google Tag Manager (noscript) -->
    <noscript>
      <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-PF46GT3" height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->

    <div class="wootric-doc-topbar">
      <div class="wootric-doc-topbar__logo">
      <% unless current_page.url == '/' %>
        <a href="/" title="Wootric Documentation Home"></a>
      <% else %>
        <a href="//wootric.com" title="Wootric Home"></a>
      <% end %>
      </div>
      <% unless current_page.url == '/' %>
        <div class="wootric-doc-topbar__docs-nav">
          <div class="wootric-doc-topbar__docs-nav__header">Documentation <span class="wootric-doc-topbar__docs-nav__header__icn"></span></div>
          <div class="wootric-doc-topbar__docs-nav__dropdown-caret"></div>
          <div class="wootric-doc-topbar__docs-nav__dropdown">
            <a href="/javascript" <% if current_page.url == '/javascript/' %>class="active"<% end %>>Javascript SDK</a>
            <a href="/segment" <% if current_page.url == '/segment/' %>class="active"<% end %>>Segment</a>
            <a href="/ios" <% if current_page.url == '/ios/' %>class="active"<% end %>>iOS SDK</a>
            <a href="/android" <% if current_page.url == '/android/' %>class="active"<% end %>>Android SDK</a>
            <a href="http://help.wootric.com/email-surveys">Email Templates</a>
            <a href="/api" <% if current_page.url == '/api/' %>class="active"<% end %>>REST API</a>
            <a href="/webhooks" <% if current_page.url == '/webhooks/' %>class="active"<% end %>>Outgoing Webhooks</a>
          </div>
        </div>
      <% end %>
      <div class="wootric-doc-topbar__space"></div>
      <div class="wootric-doc-topbar__help-link"><a href="http://help.wootric.com" target="_blank">help center</a></div>
      <div class="wootric-doc-topbar__sign">
        <a class="wootric-doc-topbar__sign-in" href="https://app.wootric.com/sign_in">sign in</a>
        <a class="wootric-doc-topbar__sign-up" href="https://app.wootric.com/sign_up">sign up free</a>
      </div>
    </div>

    <% unless current_page.url == '/' %>
      <a href="#" id="nav-button">
        <span>
          NAV
          <%= image_tag('navbar.png') %>
        </span>
      </a>
      <div class="tocify-wrapper">
        <% if language_tabs %>
          <div class="lang-selector">
            <% language_tabs.each do |lang| %>
              <% if lang.is_a? Hash %>
                <a href="#" data-language-name="<%= lang.keys.first %>"><%= lang.values.first %></a>
              <% else %>
                <a href="#" data-language-name="<%= lang %>"><%= lang %></a>
              <% end %>
            <% end %>
          </div>
        <% end %>
        <% if current_page.data.search %>
          <div class="search">
            <input type="text" class="search" id="input-search" placeholder="Search">
          </div>
          <ul class="search-results"></ul>
        <% end %>
        <div id="toc" class="wootric-tocify">
          <div class="wootric-tocify-main-header">
            <% if current_page.url == '/javascript/' %>
              Javascript SDK
            <% elsif current_page.url == '/segment/' %>
              Segment
            <% elsif current_page.url == '/ios/' %>
              iOS SDK
            <% elsif current_page.url == '/android/' %>
              Android SDK
            <% elsif current_page.url == '/api/' %>
              REST API
            <% elsif current_page.url == '/webhooks/' %>
              Outgoing Webhooks
            <% end %>
          </div>
        </div>
        <% if current_page.data.toc_footers %>
          <ul class="toc-footer">
            <% current_page.data.toc_footers.each do |footer| %>
              <li><%= footer %></li>
            <% end %>
          </ul>
        <% end %>
      </div>
    <% end %>
    <div class="page-wrapper" <% unless current_page.url != '/' %>style="margin-left: 0px;"<% end %>>
      <div class="dark-box"></div>
      <div class="content">
        <%= yield %>
        <% current_page.data.includes && current_page.data.includes.each do |include| %>
          <%= partial "includes/#{include}" %>
        <% end %>
      </div>
      <div class="dark-box">
        <% if language_tabs %>
          <div class="lang-selector">
            <% language_tabs.each do |lang| %>
              <% if lang.is_a? Hash %>
                <a href="#" data-language-name="<%= lang.keys.first %>"><%= lang.values.first %></a>
              <% else %>
                <a href="#" data-language-name="<%= lang %>"><%= lang %></a>
              <% end %>
            <% end %>
            <div class="lang-selector__border-line"></div>
          </div>
        <% end %>
      </div>
    </div>
  </body>
</html>


<!-- begin Wootric code -->
<script type="text/javascript">
  window.wootricSettings = {
    account_token: 'NPS-2a62eaf4',
    open_ended: true
  };
</script>
<script type="text/javascript" src="https://cdn.wootric.com/wootric-sdk.js"></script>
<script type="text/javascript">
  if (window.location.pathname.length > 1) {
    window.wootric('run');
  }
</script>
<!-- end Wootric code -->
